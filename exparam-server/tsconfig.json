{
    "compilerOptions": {
        "strict": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "target": "ES2021",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "module": "CommonJS",
        "jsx": "react",
        "lib": ["DOM", "ES2021", "DOM.Iterable"]
    },
    "include": ["assets/scripts/**/*"]
}
